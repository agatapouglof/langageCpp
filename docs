 # split a string into a vector  by pipe
 std::vector<std::string> tokens;
 std::string token;
 string s = "|:p:|.p.|:p:|.p.|:p:|.p.|:::|.p.|"; // string to split
 std::istringstream tokenStream(s);
 while (std::getline(tokenStream, token, '|'))
 {
    tokens.push_back(token);
 }

